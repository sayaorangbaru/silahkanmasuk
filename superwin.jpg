def updt(total, progress):
 
    barLength, status = 23, ""
    progress = float(progress) / float(total)
    if progress >= 1.:
        progress, status = 1, "\r\n"
    block = int(round(barLength * progress))
    text = "\r{} {:.0f}% {}".format(
        "\033[1;32m"+" MEMASUKAN DATA  " +"██" * block + "-" * (barLength - block), round(progress * 100, 0),
        status)
    sys.stdout.write(text)
    sys.stdout.flush()


runs = 50
for run_num in range(runs):
   time.sleep(.1)
   updt(runs, run_num + 1)

